  "license": "MIT",
  "devDependencies": {
    "allege": "^1.0.6",
    "babel-core": "^6.26.0",
    "babel-eslint": "7",
    "babel-jest": "^20.0.3",
    "babel-loader": "^6.2.4",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.16.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "camelcase": "^4.1.0",
    "codecov": "^2.3.0",
    "comment-parser": "^0.3.2",
    "css-loader": "^0.28.7",
    "expect": "^1.20.2",
    "extract-text-webpack-plugin": "^2.1.2",
    "glob": "^7.1.2",
    "jest": "^20.0.4",
    "node-sass": "^4.5.3",
    "preact": "^8.2.5",
    "prop-types": "^15.5.10",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.2",
    "uglify-es": "^3.0.28",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.7.1"
  },
  "scripts": {
    "test": "NODE_ENV=test jest --coverage && codecov",
    "build": "node ./build.js",
    "build:demo": "NODE_ENV=production webpack --config ./scripts/webpack.config.js --color --display-error-details && node ./node_modules/uglify-es/bin/uglifyjs ./docs/bundle.js -o ./docs/bundle.js",
    "watch:tests": "NODE_ENV=test jest --watchAll",
    "watch:demo": "node ./scripts/docs.js && webpack-dev-server --config ./scripts/webpack.config.js && webpack -w --config ./scripts/webpack.config.js --color --display-error-details",
    "docs": "node ./scripts/docs.js && npm run build:demo"
  },
  "babel": {
    "env": {
      "test": {
        "presets": [
          "es2017",
          "es2015",
          "stage-3"
        ],
        "plugins": [
          "transform-es2015-modules-commonjs",
          "istanbul"
        ]
      }
    }
  },
  "dependencies": {
    "babel-plugin-transform-react-jsx": "^6.24.1"
  }
}

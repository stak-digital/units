	"scripts": {
		"preversion": "npm run all && npm test",
		"all": "node scripts/units.js && npm run build && npm run docs && npm run build:demo",
		"test": "jest --coverage && codecov",
		"build": "tsc && mv ./lib/**/* ./ && mv ./lib/* ./ && find . -empty -type d -delete",
		"build:demo": "NODE_ENV=production parcel build ./docs/src/index.html -d ./docs",
		"watch:tests": "NODE_ENV=test jest --watchAll",
		"watch:demo": "node ./scripts/docs.js && parcel ./docs/src/index.html",
		"docs": "node ./scripts/docs.js && npm run build:demo",
		"start": "npm run all && npm run watch:demo"
	},
	"dependencies": {}
